set terminal pdf
set title "Event logging micro benchmark"
set output "event-logging.pdf"

set xlabel "Number of Events"
set ylabel "Time (ms)"
set offset graph 0.0, graph 0.0, graph 0.15, graph 0.0

set style line 1 lt 1 lw 7 pt 6 linecolor rgb "blue"
set style line 2 lt 1 lw 7 pt 3 linecolor rgb "red"

plot 'event-logging.dat' with lp ls 1 title 'No compression', \
     'event-logging-compress.dat' with lp ls 2 title 'With compression'

